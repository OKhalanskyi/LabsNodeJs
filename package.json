  "scripts": {
    "dev": "node dist/api/index",
    "build": "tsc --project .",
    "start": "node dist/api/index.js",
    "eslint": "eslint --ext .js,.ts --ignore-path .gitignore api/",
    "eslint:fix": "eslint --ext .js,.ts api/ --ignore-path .gitignore --fix",
